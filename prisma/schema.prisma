
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model UserAppLimit {
  id String @id @default(auto()) @map("_id") @db.ObjectId
  userid String @unique
  count Int @default(0)
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now())
}